  },
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "author": {
    "name": "Liam Doherty",
    "email": "lfdoherty@gmail.com"
  },
  "dependencies": {
    "a-big-triangle": "git+https://github.com/lfdoherty/a-big-triangle.git",
    "glslify": "^1.6.0",
    "weakmap": "0.0.6"
  },
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^5.11.2",
    "canvas-fit": "^1.2.0",
    "gl-context": "^0.1.1",
    "gl-texture2d": "^2.0.4",
    "lena": "^1.0.0"
